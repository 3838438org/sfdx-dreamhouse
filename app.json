{
    "name": "Dreamhouse Force.com Demo",
    "description": "Dreamforce Force.com Applicaiton demonstrating Force.com and Heroku Integration",
    "repository": "https://github.com/forcedotcom/dreamhouse-acdx",
    "logo": "https://avatars0.githubusercontent.com/u/464563?v=3&s=200",
    "keywords": [
        "salesforce",
        "force.com",
        "heroku"
    ],
    "addons": [
        "salesforce"
    ],
    "buildpacks": [
        {
            "url": "https://github.com/heroku/salesforce-buildpack"
        }
    ],
    "env": {
        "HEROKU_APP_NAME": {
            "description": "Provided by the Heroku platform; only set when required:true",
            "required": true
        },
        "SALESFORCE_BUILDPACK_VERBOSE": {
            "description": "Turns on additional buildpack logging.",
            "value": "true"
        },
        "SALESFORCE_HUB_URL": {
            "description": "Required for Review apps to create orgs",
            "required": true
        }
    }
}
